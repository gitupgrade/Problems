//link: https://leetcode.com/problems/find-first-and-last-position-of-element-in-sorted-array/description/
class Solution {
    public int[] searchRange(int[] nums, int target) {
        int[] a = nums;
        int low = 0;
        int high = a.length - 1;
        int x = target;
        int ans0 = -1;
        while (low <= high) {
            int mid = (low + high) / 2;
            if (a[mid] >= x) {
                if(a[mid] == x)
                {
                ans0 = mid;}
                high = mid - 1;

            } else {
                low = mid + 1;
            }
        }


        low = 0;
        high = a.length - 1;
        
        int  ans1 = -1;

        while (low <= high) {
            int mid = (low + high) / 2;
            if (a[mid] <= x) {
                if(a[mid] == x)
                {
                ans1 = mid;}
               
               low=mid+1;

            } else {
                high=mid-1;
            }
        }
        int[] b={ans0,ans1};
        return b;
        
    }
}
