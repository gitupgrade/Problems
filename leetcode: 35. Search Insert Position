//link: https://leetcode.com/problems/search-insert-position/description/

class Solution {
    public int searchInsert(int[] nums, int target) {
        
       
        int low = 0;
        int high = nums.length - 1;
        int x = target;
        int ans = nums.length;
        while (low <= high) {
            int mid = (low + high) / 2;
            if (nums[mid] >= x) {
                ans = mid;
                high = mid - 1;

            } else {
                low = mid + 1;
            }
        }
       return ans;



    }
}
