//link: https://www.codechef.com/START208D/problems/ALJMP
#include <iostream>
using namespace std;
int main()
{   int T;
    cin>>T;
    while(T--)
    {
        int n;
        cin>>n;
        int p=n;
        int j=1;
        for(int i=n-1;i>=1;i--)
        {   
            if(j%2==0)
            {
                      p=p+i;
            }
            else{
                p=p-i;
            }
            j=j+1;
        }
        cout<<p<<endl;
    }
    return 0;
}
