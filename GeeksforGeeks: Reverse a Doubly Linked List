//link: https://www.geeksforgeeks.org/problems/reverse-a-doubly-linked-list/1
/*
class Node {
  public:
    int data;
    Node *next;
    Node *prev;
    Node(int val) {
        data = val;
        next = NULL;
        prev = NULL;
    }
};

*/
class Solution {
  public:
    Node *reverse(Node *head) {
        if(head==NULL||head->next==NULL)
    {
        return head;;
    }

Node* myprev =NULL;
Node* current=head;
while(current!=NULL){
    myprev=current->prev;
    current->prev=current->next;
      current->next=myprev;
      current=current->prev;

}
return myprev->prev;
        
    }
};
