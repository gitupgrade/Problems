//link: https://www.codechef.com/problems/PSAS?tab=statement
#include <bits/stdc++.h>

using namespace std;

int main() {
    int T;
    cin >> T;
    while (T--)
    {
        bool flag = true;
        int N;
        cin >> N;
        vector < int > a(N);
        for (int i = 0; i < N; i++)
        {
            cin >> a[i];
        }
        int c = 0;
        for (int i = 0; i < (N - 1); i++)
        {
            if (abs(a[i]) != abs(a[i + 1]))
            {
                
                flag = false;
                break;
            }
            if ((a[i] < 0 && a[i + 1] > 0) || (a[i] > 0 && a[i + 1] < 0))
            {
                c++;
            }
            if (c > 1)
            {
                
                flag = false;
                break;
            }


        }
        if (flag) cout << "YES\n";

        else cout << "NO\n";

    }



    return 0;
}
