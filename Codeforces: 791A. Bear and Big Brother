//link:https://codeforces.com/problemset/problem/791/A
import java.util.Scanner;

public class limak {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int l = sc.nextInt();
        int b = sc.nextInt();
        sc.close();
        for (int i = 1;; i++) {
            l = l * 3;
            b = b * 2;
            if (l > b) {
                System.out.println(i);
                break;
            }
        }
    }
}
